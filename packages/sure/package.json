{
  "name": "@robotsx/sure",
  "version": "0.0.7",
  "type": "module",
  "exports": "./esm/index.js",
  "types": "./src/index.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "prepublish": "npm run build",
    "clean": "pnpm run clean:remove-generated",
    "clean:remove-generated": "find ./src -type f -name '*.js' -delete && find ./src -type f -name '*.d.ts' -delete",
    "dev": "tsc --watch",
    "lint": "eslint \"src/**/*.ts*\" --quiet",
    "lint:warn": "eslint \"src/**/*.ts*\"",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "devDependencies": {
    "@types/jest": "29.5.5",
    "eslint": "8.51.0",
    "eslint-config-custom": "workspace:*",
    "jest": "29.7.0",
    "jest-presets": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "5.3.0-beta"
  }
}
