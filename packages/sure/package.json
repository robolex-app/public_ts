{
  "name": "@robolex/sure",
  "version": "0.7.2",
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "types": "./src/index.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.build.json",
    "prepublish": "npm run build",
    "clean": "rm -rf ./esm",
    "dev": "tsc --watch --project tsconfig.build.json",
    "tsc:ci": "tsc --noEmit",
    "tsc": "tsc --noEmit --watch",
    "lint": "eslint \"src/**/*.ts*\" --quiet",
    "lint:warn": "eslint \"src/**/*.ts*\"",
    "test:w": "vitest run",
    "test:ui": "vitest --ui",
    "test": "vitest"
  },
  "devDependencies": {
    "eslint": "8.51.0",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "5.3.0-beta",
    "vitest": "^0.34.6"
  }
}
